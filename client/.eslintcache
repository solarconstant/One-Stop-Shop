 }, []\r\n    )\r\n\r\n    const handleSubmit = async (e) =>\r\n    {\r\n        e.preventDefault();\r\n        //Email and password validation\r\n        if(!email || !password)\r\n        {\r\n            toast.error(\"Please enter your password.\");\r\n            return;\r\n        }\r\n\r\n        if(password.length < 8)\r\n        {\r\n            toast.error(\"Your password must be atleast 8 characters long.\");\r\n            return;\r\n        }\r\n        try\r\n        {\r\n            const result = await auth.signInWithEmailLink(email, window.location.href);     //location.href returns url of current page\r\n            //console.log('RESULT->', result);\r\n\r\n            if(result.user.emailVerified)\r\n            {\r\n                // Once user is verified we can remove creds from local storage\r\n                window.localStorage.removeItem('emailForRegistration');\r\n                // User ID Token\r\n                let user = auth.currentUser;\r\n                await user.updatePassword(password);\r\n                const idTokenResult = await user.getIdTokenResult();\r\n\r\n                createOrUpdateUser(idTokenResult.token)\r\n                .then(\r\n                    (res) =>\r\n                    {\r\n                        dispatch(\r\n                            {\r\n                                type: \"LOGGED_IN_USER\",\r\n                                payload: {\r\n                                    name: res.data.name,\r\n                                    email: res.data.email,\r\n                                    token: idTokenResult.token,\r\n                                    role: res.data.role,\r\n                                    _id: res.data._id,\r\n                                },\r\n                            }\r\n                        );\r\n                    } \r\n                )\r\n                .catch(err => console.log(err));\r\n                //redirect\r\n                history.push('/');\r\n            }\r\n        }\r\n        catch(error)\r\n        {\r\n            console.log(error);\r\n            toast.error(error.message)\r\n        }\r\n    }\r\n\r\n    const completeRegistrationForm = () =>(\r\n        <form onSubmit = {handleSubmit}>\r\n            <input type = \"email\" className = \"form-control\" value = {email} onChange = {e => setPassword(e.target.value)} disabled />\r\n            <input type = \"password\" className = \"form-control\" value = {password} onChange = {e => setPassword(e.target.value)} autoFocus />\r\n            <button type = \"submit\" className = \"btn btn-raised\">\r\n                Complete Registration\r\n            </button>\r\n        </form>\r\n    );\r\n    return (\r\n        <div className = \"container p-5\">\r\n            <div className = \"row\">\r\n                <div className = \"col-md-6 offset-md-3\">\r\n                    <h4>\r\n                        Register Completion \r\n                    </h4>\r\n                    {completeRegistrationForm()} \r\n                </div>\r\n            </div>\r\n        </div>\r\n    )\r\n}\r\n\r\nexport default RegisterCompletion;","D:\\Web Development\\Ecommerce Project\\client\\src\\Components\\Navbar\\Header.js",[],"D:\\Web Development\\Ecommerce Project\\client\\src\\Reducers\\userReducer.js",[],"D:\\Web Development\\Ecommerce Project\\client\\src\\Firebase.js",[],"D:\\Web Development\\Ecommerce Project\\client\\src\\Functions\\auth.js",[],"D:\\Web Development\\Ecommerce Project\\client\\src\\App.js",["74"],"D:\\Web Development\\Ecommerce Project\\client\\src\\Pages\\User\\History.js",[],"D:\\Web Development\\Ecommerce Project\\client\\src\\Components\\Routes\\UserRoute.js",["75"],"D:\\Web Development\\Ecommerce Project\\client\\src\\Components\\Routes\\LoadingToRedirectUser.js",["76"],"D:\\Web Development\\Ecommerce Project\\client\\src\\Components\\Navbar\\UserNav.js",[],"D:\\Web Development\\Ecommerce Project\\client\\src\\Pages\\Us